<h1>/Vote</h1>

<p>
  <b>Question</b><br>
  <%= @vote.question_id %>
</p>

<p>
  <b>User</b><br>
  <%= @vote.user_id %>
</p>

<div class="form-actions">
  <%= link_to 'Back', votes_path, :class => 'btn'  %>
  <%= link_to 'Edit', edit_vote_path(@vote), :class => 'btn' %>
  <%= link_to 'Delete', vote_path(@vote), :method => 'delete', :confirm => 'Are you sure?', :class => 'btn btn-danger' %>
</div>
