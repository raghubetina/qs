<h1>Votes</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>Question</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @votes.each do |vote| %>
      <tr>
        <td><%= vote.id %></td>
        <td><%= link_to vote.question_id, vote_path(vote) %></td>
        <td><%= vote.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_vote_path(vote), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', vote_path(vote), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_vote_path, :class => 'btn btn-primary' %>
